{
    "name": "todstoychev/form-errors-normalizer",
    "description": "Symfony Form object errors normalizer. Suitable to use for building responses for RESTful API's.",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "symfony/validator": ">=3.0",
        "symfony/form": ">=3.0",
        "symfony/serializer": ">=3.0"
    },
    "require-dev": {
        "symfony/var-dumper": ">=3.0",
        "phpunit/phpunit": ">=6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Todor Todorov",
            "email": "todstoychev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Todstoychev\\FormErrorsNormalizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Todstoychev\\FormErrorsNormalizer\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
